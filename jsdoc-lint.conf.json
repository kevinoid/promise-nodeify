{
  "//": "JSDoc configuration for linting",
  "opts": {
    "access": "all",
    "destination": "doc/api",
    "pedantic": true,
    "recurse": true
  },
  "source": {
    "exclude": [
      "benchmark/node_modules",
      "coverage",
      "dist",
      "doc",
      "node_modules"
    ]
  },
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  }
}
